{% extends 'base_auth.html' %}

{% block body %}

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>User Type</th>
            <th>Created At</th>
            <th>Updated At</th>
           
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>{{ user['user_id']}}</td>
            <td>{{ user['name']}}</td>
            <td>{{ user['surname']}}</td>
            <td>{{ user['email']}}</td>
            <td>{{ user['phone_number']}}</td>
            <td>{{ user['user_type']}}</td>
            <td>{{ user['created_at']}}</td>
            <td>{{ user['updated_at']}}</td>
        </tr>
    </tbody>
</table>

{% endblock %}
